    "highlight.js": "9.12.0",
    "intl": "1.2.5",
    "moment": "2.19.4",
    "ngx-color-picker": "5.2.0",
    "ngx-cookie-service": "1.0.9",
    "perfect-scrollbar": "1.3.0",
