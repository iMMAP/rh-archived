    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-aot": "ng build --prod && electron .",
    "pack": "build",
    "build": "ng build --prod && build"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.2",
